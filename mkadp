#!/system/bin/sh
#=======================================#
#VARIABLES===============================#
BB=/data/adb/magisk/busybox
#=======================================#
#=======================================#

echo "Turning on Extreme Mode if its not already"
sleep 2

$BB cp -rf /data/adb/modules/butterlimitsforop7poos11uni/blext.sh /data/adb/modules/butterlimitsforop7poos11uni/service.sh
$BB cp -rf /data/adb/modules/butterlimitsforop7poos11uni/system/etc/mkadp/extreme/module.prop /data/adb/modules/butterlimitsforop7poos11uni
$BB cp -rf /data/adb/modules/butterlimitsforop7poos11uni/system/etc/mkadp/extreme/system.prop /data/adb/modules/butterlimitsforop7poos11uni

sleep 5
reboot
